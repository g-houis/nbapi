    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
