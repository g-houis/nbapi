{
  "name": "nba-api-client",
  "version": "0.0.1",
  "description": "Typescript api to fetch data from nba web services",
  "main": "index.ts",
  "repository": "https://github.com/g-houis/nbapi.git",
  "author": "guillaume houis <guillaume.houis@gmail.com>",
  "license": "Apache 2",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "axios": "^1.3.4"
  },
  "scripts": {
    "build": "yarn tsc:cjs && yarn tsc:esm",
    "tsc:cjs": "tsc --module commonjs --outDir build/cjs -p tsconfig.json",
    "tsc:esm": "tsc --module esnext --outDir build/esm -p tsconfig.json",
    "test": "jest --config jest.config.ts",
    "eslint": "eslint src --ext .ts"
  },
  "devDependencies": {
